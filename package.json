{
  "name": "semantic-release-codeartifact",
  "version": "0.0.0-development",
  "description": "semantic-release plugin for AWS CodeArtifact",
  "main": "./lib/src/index.js",
  "files": [
    "lib/**/*"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ryansonshine/semantic-release-codeartifact.git"
  },
  "homepage": "https://github.com/ryansonshine/semantic-release-codeartifact#readme",
  "bugs": {
    "url": "https://github.com/ryansonshine/semantic-release-codeartifact/issues"
  },
  "contributors": [
    {
      "name": "Ryan Sonshine",
      "url": "https://github.com/ryansonshine"
    }
  ],
  "engines": {
    "node": ">=14.17"
  },
  "keywords": [
    "aws",
    "codeartifact",
    "semver",
    "package",
    "publish",
    "automation",
    "semantic versioning",
    "semantic-release"
  ],
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf ./lib/",
    "cm": "cz",
    "coverage": "codecov",
    "lint": "eslint ./src/ --fix",
    "prepare": "husky install",
    "semantic-release": "semantic-release",
    "test:watch": "jest --watch",
    "test": "jest --coverage",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@aws-sdk/client-codeartifact": "^3.13.1",
    "@semantic-release/error": "^2.2.0",
    "aggregate-error": "^3.1.0",
    "aws-sdk-v3-proxy": "^2.0.5",
    "fs-extra": "^10.0.0",
    "read-pkg": "5.2.0",
    "save": "^2.4.0"
  },
  "peerDependencies": {
    "semantic-release": "^18.0.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^9.0.11",
    "@types/jest": "^26.0.24",
    "@types/node": "^12.20.11",
    "@types/signale": "^1.4.1",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "aws-sdk-client-mock": "^0.4.0",
    "codecov": "^3.8.1",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.25.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^6.0.0",
    "jest": "^27.2.0",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "semantic-release": "^18.0.0",
    "signale": "^1.4.0",
    "tempy": "^1.0.1",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.2.1",
    "typescript": "^4.2.4"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.ts": "eslint --cache --cache-location .eslintcache --fix"
  },
  "release": {
    "branches": [
      "main"
    ]
  }
}
